<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:kim="http://www.liquibase.org/xml/ns/changelog-ext"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="tst-assign-role-member" author="lb-ext" context="default">
        <customChange class="liquibase.change.ext.AssignRoleMember">
            <param name="namespace" value="KUALI"/>
            <param name="role" value="User2"/>
            <param name="type" value="P"/>
            <param name="member" value="kr"/>
        </customChange>
    </changeSet>

    <changeSet id="tst-assign-role-member-cns" author="lb-ext" context="custom-namespace">
        <kim:roleMember namespace="KUALI" role="User2" type="P" member="kr">
            <kim:roleMemberAttribute type="Default" value="Test Role Mbr Attr" attributeDef="beanName"/>
            <kim:roleMemberAttribute type="Default" value="Unique Value" attributeDef="beanName" unique="true" />
        </kim:roleMember>
    </changeSet>

</databaseChangeLog>